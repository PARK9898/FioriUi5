<mvc:View controllerName="exercise0809.controller.Detail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page title="{i18n>title}"
        showNavButton= "true"
        navButtonPress="onGoBack">
        <content>
            <VBox>
                <Text text = "{detailModel>/no}"/>
                <Text text = "{detailModel>/name}"/>
                <Text text = "{detailModel>/gender}"/>
                <Text text = "{detailModel>/birthdate}"/>
            </VBox>
        </content>

    </Page>
</mvc:View>